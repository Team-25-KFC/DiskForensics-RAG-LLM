[
  {
    "category": "Persistence",
    "subcategory": "BHO_Register",
    "description": "Browser Helper Object(BHO)를 등록해 IE/Explorer 프로세스에 DLL을 로드되게 한다.",
    "keywords": [
      "Microsoft\\Windows\\CurrentVersion\\Explorer\\Browser Helper Objects\\{GUID} AND (Default)",
      "CLSID\\{GUID}\\InprocServer32 AND (Default)"
    ]
  },
  {
    "category": "Persistence",
    "subcategory": "ShellExecuteHooks",
    "description": "ShellExecuteHooks에 CLSID를 추가해 프로세스 실행 시 DLL이 개입되게 한다.",
    "keywords": [
      "Microsoft\\Windows\\CurrentVersion\\Explorer\\ShellExecuteHooks AND {GUID}",
      "CLSID\\{GUID}\\InprocServer32 AND (Default)",
      "InprocServer32 AND C:\\ProgramData\\%"
    ]
  },
  {
    "category": "Persistence",
    "subcategory": "ShellExtensions_ContextMenu",
    "description": "컨텍스트 메뉴 핸들러를 등록해 Explorer가 DLL을 로드되게 한다.",
    "keywords": [
      "shellex\\ContextMenuHandlers AND (Default)",
      "CLSID\\{GUID}\\InprocServer32 AND (Default)"
    ]
  },
  {
    "category": "Persistence",
    "subcategory": "ShellExtensions_PropertySheet",
    "description": "속성 시트 핸들러를 등록해 속성 창 열람 시 DLL이 로드되게 한다.",
    "keywords": [
      "shellex\\PropertySheetHandlers AND (Default)",
      "CLSID\\{GUID}\\InprocServer32 AND (Default)"
    ]
  },
  {
    "category": "Persistence",
    "subcategory": "ShellExtensions_Approved",
    "description": "Approved 목록을 조작해 셸 확장을 허용되게 한다.",
    "keywords": [
      "Microsoft\\Windows\\CurrentVersion\\Shell Extensions\\Approved AND {GUID}"
    ]
  },
  {
    "category": "Persistence",
    "subcategory": "ShellExtensions_DropTarget_DragDrop",
    "description": "DropHandler/DragDropHandlers 등록으로 파일 드롭 시 DLL이 로드되게 한다.",
    "keywords": [
      "shellex\\DropHandler AND (Default)",
      "AllFileSystemObjects\\shellex\\DragDropHandlers AND (Default)",
      "CLSID\\{GUID}\\InprocServer32 AND (Default)"
    ]
  },
  {
    "category": "Persistence",
    "subcategory": "PerUser_COM_Override",
    "description": "HKCU\\Software\\Classes로 사용자 단위 CLSID를 등록해 시스템 CLSID를 우선 대체되게 한다.",
    "keywords": [
      "CLSID\\{GUID} AND InprocServer32",
      "CLSID\\{GUID} AND LocalServer32"
    ]
  },
  {
    "category": "Persistence",
    "subcategory": "Shell_BandObjects_Toolbars",
    "description": "Explorer 툴바/밴드 오브젝트를 등록해 Explorer 실행 시 로드되게 한다.",
    "keywords": [
      "Microsoft\\Windows\\CurrentVersion\\Explorer\\Toolbar\\Explorer AND {GUID}",
      "CLSID\\{GUID}\\InprocServer32 AND (Default)"
    ]
  }
]
